<!DOCTYPE html>
<html class="{{ctmCoreClass}}">
	<head>
		<title>{{ title }}</title>

		<link rel="stylesheet" href="{{assetsBasePath}}/build/main.css" />

		{{> ctm}}

		<script src="{{polyfillServiceUrl}}"></script>

		<script>
			(function () {
				ctmLoadScript({
					src: "{{assetsBasePath}}/build/main.js",
					async: true
				});
			}());
		</script>
	</head>
	<body>
		{{#headerConfig}}
			{{> header}}
		{{/headerConfig}}

		<div class="o-grid-container">
			<div class="o-grid-row">
				<div data-o-grid-colspan="12">
					{{> body}}
				</div>
			</div>
		</div>
	</body>
</html>
